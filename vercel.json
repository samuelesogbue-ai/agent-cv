{
    "buildCommand": "pnpm build",
    "outputDirectory": "frontend/.next",
    "devCommand": "pnpm dev",
    "installCommand": "pnpm install && pip install -r requirements.txt",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        }
    ]
}